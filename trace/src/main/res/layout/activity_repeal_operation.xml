<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RevokeOperationActivity">

    <com.yunsoo.view.TitleBar
        android:id="@+id/repeal_title_bar"
        layout="@layout/title_bar_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true" />

    <LinearLayout
        android:layout_below="@+id/repeal_title_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include
            android:id="@+id/rl_repeal_inbound"
            layout="@layout/action_item_layout_bottom" />

        <include
            android:id="@+id/rl_repeal_outbound"
            layout="@layout/action_item_layout_bottom" />

    </LinearLayout>

</RelativeLayout>
